<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/White">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/addressInfo"
        android:padding="15dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="나"
            android:textSize="20sp"
            android:textColor="@color/colorAccent"
            android:layout_alignParentTop="true" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/numOfAddress"
            android:text="132개의 연락처"
            android:textSize="20sp"
            android:textColor="@color/Dark"
            android:layout_marginLeft="180dp"/>
    </LinearLayout>

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:id="@+id/myinfo"
        android:layout_height="105dp"
        android:layout_below="@id/addressInfo"
        android:padding="10dp"
        android:background="@drawable/border_below_shadow">

        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:id="@+id/profile"
            android:layout_alignParentLeft="true"
            android:src="@drawable/ic_profile"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/user_name"
            android:text="문재수"
            android:textSize="25sp"
            android:textColor="@color/Black"
            android:layout_toRightOf="@+id/profile"
            android:layout_marginTop="4dp"
            android:layout_marginLeft="10dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/phone_number"
            android:text="010-1234-5678"
            android:textColor="@color/Black"
            android:layout_below="@+id/user_name"
            android:layout_toEndOf="@+id/profile"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="4dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="4dp"
            android:text="theminjoo@naver.com"
            android:textColor="@android:color/darker_gray"
            android:id="@+id/user_email"
            android:layout_below="@+id/phone_number"
            android:layout_toRightOf="@+id/profile"
            />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/startlist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/myinfo">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:textSize="20sp"
            android:textColor="@color/colorAccent"
            android:text="#"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_below="@id/startlist">

        <ListView
            android:id="@+id/addressList"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

        </ListView>
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.melnykov.fab.FloatingActionButton
            android:id="@+id/btn_plus"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginRight="50dp"
            android:layout_gravity="bottom|right"
            android:background="@drawable/ic_plus" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:id="@+id/consonantBtn"
        android:layout_alignParentRight="true"
        android:layout_margin="10dp"
        android:weightSum="40">
        <Button
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="#"
            android:textSize="15sp"
            android:textColor="@android:color/darker_gray"/>
        <Button
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="ㄱ"
            android:textSize="15sp"
            android:textColor="@android:color/darker_gray"/>
        <Button
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="ㄴ"
            android:textSize="15sp"
            android:textColor="@android:color/darker_gray"/>
        <Button
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="ㄷ"
            android:textSize="15sp"
            android:textColor="@android:color/darker_gray"/>
        <Button
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="ㄹ"
            android:textSize="15sp"
            android:textColor="@android:color/darker_gray"/>
        <Button
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="ㅁ"
            android:textSize="15sp"
            android:textColor="@android:color/darker_gray"/>
        <Button
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="ㅂ"
            android:textSize="15sp"
            android:textColor="@android:color/darker_gray"/>
        <Button
            android:layout_width="15dp"
